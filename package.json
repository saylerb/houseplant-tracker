{
  "name": "houseplant-tracker",
  "version": "1.0.0",
  "scripts": {
    "start:prod": "npm run migrate && node __sapper__/build",
    "start": "node __sapper__/build",
    "build": "sapper build --legacy",
    "migrate": "prisma migrate up --experimental",
    "dev": "sapper dev",
    "export": "sapper export --legacy",
    "test": "jest src",
    "format": "prettier --write \"./!(__sapper__)/**/*.{js,json,svelte,md,html,css}\"",
    "format:check": "prettier --check \"./!(__sapper__)/**/*.{js,json,svelte,md,html,css}\"",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "engines": {
    "node": "14.x"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/svelte": "^1.11.2",
    "@tsconfig/svelte": "^1.0.10",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.10",
    "@types/polka": "^0.5.1",
    "babel-jest": "^25.5.1",
    "express-serve-static-core": "^0.1.1",
    "jest": "^25.5.4",
    "jest-transform-svelte": "^2.1.1",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.4.1",
    "rollup": "^2.32.0",
    "sapper": "^0.28.0",
    "svelte": "^3.29.0",
    "svelte-check": "^1.1.6",
    "svelte-preprocess": "^4.3.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@prisma/cli": "^2.8.1",
    "@prisma/client": "^2.8.1",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie": "^0.4.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "polka": "^1.0.0-next.11",
    "proposal-temporal": "^0.4.0",
    "rollup-plugin-svelte": "^6.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sirv": "^1.0.7",
    "sirv-cli": "^0.4.6"
  }
}
