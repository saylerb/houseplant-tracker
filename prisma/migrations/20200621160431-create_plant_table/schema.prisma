datasource postgresql {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}


model plant {
  id Int @default(autoincrement()) @id
  name String
  description String?
  lastWateredAt DateTime @default(now())
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
